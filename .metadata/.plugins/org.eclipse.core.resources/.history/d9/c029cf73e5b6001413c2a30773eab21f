	private ValueStoreImpl valueStore;
	private int intKey;
	@Before
	public void initialize(){
		valueStore = new ValueStoreImpl();
	}

	@Test
	public void testPut() {
		
		intKey = 1;
		File entry = new File(valueStore.getWorkingDirectory() + intKey + ".txt");
		
		if(entry.exists()){
			entry.delete();
		}
		
		
	}

}
